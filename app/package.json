{
  "name": "itsgoingtobe-react",
  "version": "0.0.1",
  "engines": {
    "node": "12.16.x"
  },
  "description": "It's Going To Be front-end with React, Redux, and React-Router!",
  "main": "main.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node scripts/start",
    "build": "cross-env PUBLIC_URL=/client node scripts/build",
    "test": "node scripts/test --env=jsdom",
    "lint": "eslint --ext js,jsx src/.",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006 -c .storybook",
    "profile": "cross-env NODE_ENV=production BABEL_ENV=production GENERATE_SOURCEMAP=false webpack --config config/webpack.config.prod.js --profile --json > webpack-stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sproogen/itsgoingto.be"
  },
  "author": "James Grant <james@jwgmedia.co.uk> (http://jameswgrant.co.uk)",
  "license": "GPLv3",
  "dependencies": {
    "antd": "^3.26.12",
    "autoprefixer": "^8.6.5",
    "autosize": "^4.0.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-ramda": "^1.6.3",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "cancelable-promise": "^2.5.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.1",
    "css-loader": "^0.28.7",
    "dotenv": "^5.0.1",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.18.3",
    "extract-text-webpack-plugin": "3.0.2",
    "fbemitter": "^2.1.1",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "promise": "^8.0.3",
    "prop-types": "^15.7.2",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "react": "^16.12.0",
    "react-cookie": "^2.2.0",
    "react-countdown-now": "^1.2.0",
    "react-datepicker": "^0.53.0",
    "react-dev-utils": "^5.0.3",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-linkify": "^0.2.1",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.1.2",
    "react-router": "^3.2.5",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "resolve": "^1.15.1",
    "sass-loader": "^7.3.1",
    "socket.io-client": "^2.3.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.1.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.12",
    "babel-jest": "^22.4.4",
    "codacy-coverage": "^3.4.0",
    "jest": "^22.4.4",
    "should": "^13.2.3",
    "should-enzyme": "^2.1.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "jest": {
    "coverageDirectory": "build/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/scripts/",
      "<rootDir>/.storybook/",
      "<rootDir>/stories/",
      "<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).{js,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-tests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
