{
  "name": "itsgoingtobe-server",
  "version": "0.0.1",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "app.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "babel app.js -d dist",
